@model IBIN.DAL.Chemical
@{
    ViewBag.Title = "AddSpecies";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Add Chemical</h2>
@using (Html.BeginForm("AddChemical", "Admin", FormMethod.Post, new { enctype = "multipart/form-data", @id = "formCarType", @class = "form-horizontal" }))
{



    <div class="form-group">
        @Html.LabelFor(model => model.ChemicalName, new { @class = "col-md-2 control-label" })
        <div class="col-md-3">
            @Html.TextBoxFor(model => model.ChemicalName, new { @class = "form-control input-lg" })


        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.FileName, new { @class = "col-md-2 control-label" })

        <div class="col-md-3">
            <input type="file" id="SpeciesFile" name="SpeciesFile" />
        </div>

        <div class="col-md-3">
<input type="submit" value="Save" />
        </div>

    </div>
    
    @Html.HiddenFor(x=>x.ChemicalId)
}
